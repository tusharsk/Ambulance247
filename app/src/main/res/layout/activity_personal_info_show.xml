<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tusharsk.ambulance247.PersonalInfo_show">

    <TextView
        android:id="@+id/tvgender"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="25dp"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvemergency" />

    <TextView
        android:id="@+id/tvage"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="TextView"
        android:layout_marginTop="33dp"
        app:layout_constraintTop_toBottomOf="@+id/tvmobile"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/tvmobile"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="TextView"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/tvbloodgroup"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/tvbloodgroup"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="TextView"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/tvdob"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/tvdob"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="TextView"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/tvname"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="35dp"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/tvname" />

    <TextView
        android:id="@+id/tvemergency"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="TextView"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/tvage"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />


</android.support.constraint.ConstraintLayout>
